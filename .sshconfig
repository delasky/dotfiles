### Development
# plt-dev
    Host *.plt.rbcplatform.com
    ControlMaster auto
    ControlPath ~/.ssh/%C
    ControlPersist yes
    ServerAliveInterval 60
    Host plt-dev-*.plt.rbcplatform.com
    ProxyCommand ssh bastion-2fa.plt.rbcplatform.com 'nc -w 600 %h %p'

#dev
Host *.rbcplatform.com
    ControlMaster auto
    ControlPath ~/.ssh/%C
    ControlPersist 8h
    ServerAliveInterval 60
    User dolasky
    Host dev*.useast1.rbcplatform.com
    ProxyCommand ssh bastion-2fa.useast1.rbcplatform.com 'nc -w 600 %h %p'

