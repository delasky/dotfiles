[user]
	name = Daniel Olasky
	email = daniel.olasky@hotschedules.com
[push]
	default = current
[tag]
    sort = version:refname
[alias]
    sync = !git checkout staging && git pull && git checkout @{-1} && git rebase staging
    co = checkout
[pull]
    rebase = preserve
[merge]
    ff = false
[status]
    showUntrackedFiles = all
[core]
    editor = vim
    excludesfile = ~/.gitignore_global

